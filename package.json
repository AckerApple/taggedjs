{
  "type": "module",
  "scripts": {
    "save": "git add . && git commit -m \"patch\" && git push",
    "start": "run-p start:hmr",
    "start:hmr": "node --loader ts-node/esm ./hmr/hmr-server.ts --dir \"./src\"",
    "build": "run-s build:hmr build:bundle",
    "build:bundle": "node --loader ts-node/esm ./runBundleScript.ts",
    "build:watch": "node --loader ts-node/esm ./runBundleScript.ts",
    "build:js": "rm -rf assets/js && mkdir assets/js && tsc -p tsconfig.build.json",
    "build:hmr": "node hmr/runBundleScript.hmr.mjs",
    "build:test": "ts-node src/testbuild.ts -p tsconfig.build.json",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "update:fx": "cp -R ../../taggedjs-animate-css/main/dist/. ./node_modules/taggedjs-animate-css",
    "copy:taggedjs": "rm -rf ./todo/node_modules/taggedjs && cp -R ./node_modules/taggedjs ./todo/node_modules/"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.10",
    "@vitest/browser": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "compression-webpack-plugin": "^11.1.0",
    "express": "^4.18.2",
    "glob": "^11.0.3",
    "happy-dom": "^18.0.1",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.53.2",
    "rxjs": "^7.8.2",
    "serve-static": "^1.15.0",
    "taggedjs-cli": "^0.1.19",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "ws": "^8.15.1"
  },
  "dependencies": {
    "taggedjs": "^2.8.43",
    "taggedjs-animate-css": "^0.0.16"
  }
}
